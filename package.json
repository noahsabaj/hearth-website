{
  "name": "hearth-website",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://noahsabaj.github.io/hearth-website",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^2.0.1",
    "framer-motion": "^12.18.1",
    "fuse.js": "^7.1.0",
    "puppeteer": "^24.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:quiet": "eslint src --ext .ts,.tsx,.js,.jsx --quiet",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "clean": "rm -rf build dist node_modules/.cache",
    "dev": "npm start",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test:coverage",
    "prepare": "npm run validate",
    "test:unit": "react-scripts test --coverage=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:component": "cypress run --component",
    "test:visual": "npm run test -- --testPathPattern=visual",
    "test:a11y": "node scripts/generate-a11y-report.js",
    "test:performance": "node scripts/performance-benchmarks.js",
    "test:all": "npm run test:coverage && npm run test:e2e && npm run test:a11y && npm run test:performance",
    "test:cross-browser": "node scripts/cross-browser-tests.js",
    "test:browsers": "npm run test:cross-browser",
    "test:compatibility": "node scripts/cross-browser-tests.js compatibility",
    "selenium:start": "docker-compose -f docker-compose.selenium.yml up -d",
    "selenium:stop": "docker-compose -f docker-compose.selenium.yml down",
    "selenium:logs": "docker-compose -f docker-compose.selenium.yml logs -f",
    "selenium:test": "node scripts/test-selenium-setup.js",
    "benchmark": "npm run test:performance",
    "audit:a11y": "npm run test:a11y",
    "ci:test": "npm run test:coverage -- --watchAll=false && npm run test:e2e && npm run test:a11y && npm run test:cross-browser"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "bash -c 'tsc --noEmit'"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@lhci/cli": "^0.15.0",
    "@percy/cli": "^1.31.0",
    "@percy/cypress": "^3.1.6",
    "@percy/puppeteer": "^2.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "bundlesize": "^0.18.2",
    "cross-env": "^7.0.3",
    "cypress": "^14.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "lighthouse-ci": "^1.13.1",
    "lint-staged": "^15.5.2",
    "msw": "^2.10.2",
    "pa11y": "^9.0.0",
    "pa11y-ci": "^3.1.0",
    "prettier": "^2.8.8",
    "selenium-webdriver": "^4.33.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
